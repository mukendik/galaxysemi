<project>
    <functionDefinitionList>
        <actionDefinition>
            <name>unixfnplauncher</name>
            <actionList>
                <actionGroup>
                    <ruleList>
                        <isFalse value="${installer_is_root_install}"/>
                    </ruleList>
                    <actionList>
                        <if>
                            <conditionRuleList>
                                <programTest name="gksu" condition="is_not_in_path"/>
                                <programTest name="kdesu" condition="is_not_in_path"/>
                            </conditionRuleList>
                            <actionList>
                                <showInfo>
                                    <title>FNP Components</title>
                                    <text>FNP won't be installed now. You have to start the following command as admin user: ${installdir}/fnp_utils/install_fnp.sh ${installdir}/fnp_utils/FNPLicensingService/FNPLicensingService</text>
                                </showInfo>
                            </actionList>
                            <elseActionList>
                                <runProgram program="gksu" programArguments="&quot;${installdir}/fnp_utils/install_fnp.sh&quot; &quot;${installdir}/fnp_utils/FNPLicensingService/FNPLicensingService&quot;">
                                    <ruleList>
                                        <programTest name="gksu" condition="is_in_path"/>
                                    </ruleList>
                                </runProgram>
                                <runProgram program="kdesu" programArguments="-c &quot;${installdir}/fnp_utils/install_fnp.sh&quot; &quot;${installdir}/fnp_utils/FNPLicensingService/FNPLicensingService&quot;">
                                    <ruleList>
                                        <programTest name="gksu" condition="is_not_in_path"/>
                                        <programTest name="kdesu" condition="is_in_path"/>
                                    </ruleList>
                                </runProgram>
                            </elseActionList>
                        </if>
                    </actionList>
                </actionGroup>
                <actionGroup>
                    <ruleList>
                        <isTrue value="${installer_is_root_install}"/>
                    </ruleList>
                    <actionList>
                        <runProgram>
                            <program>"${installdir}/fnp_utils/install_fnp.sh"</program>
                            <programArguments>&quot;${installdir}/fnp_utils/FNPLicensingService/FNPLicensingService&quot;</programArguments>
                        </runProgram>
                    </actionList>
                </actionGroup>
            </actionList>
            <parameterList>
                <stringParameter name="program" value="" default=""/>
                <stringParameter name="programArguments" value="" default=""/>
                <stringParameter name="workingDirectory" value="" default=""/>
                <stringParameter name="stderr" value="" default="program_stderr"/>
                <stringParameter name="stdout" value="" default="program_stdout"/>
            </parameterList>
        </actionDefinition>
        <actionDefinition>
            <name>osxfnplauncher</name>
            <actionList>
                <if>
                    <conditionRuleList>
                        <isFalse value="${installer_is_root_install}"/>
                    </conditionRuleList>
                    <actionList>
                        <showInfo>
                            <title>FNP Components</title>
                            <text>FNP won't be installed now. You have to start the following command as admin user: ${installdir}/fnp_utils/install_fnp.sh ${installdir}/fnp_utils/FNPLicensingService/FNPLicensingService</text>
                        </showInfo>
                    </actionList>
                    <elseActionList>
                        <runProgram>
                            <program>${installdir}/fnp_utils/install_fnp.sh</program>
                            <programArguments>&quot;${installdir}/fnp_utils/FNPLicensingService/FNPLicensingService&quot;</programArguments>
                        </runProgram>
                    </elseActionList>
                </if>
            </actionList>
            <parameterList>
                <stringParameter name="program" value="" default=""/>
                <stringParameter name="programArguments" value="" default=""/>
                <stringParameter name="workingDirectory" value="" default=""/>
                <stringParameter name="stderr" value="" default="program_stderr"/>
                <stringParameter name="stdout" value="" default="program_stdout"/>
            </parameterList>
        </actionDefinition>
    </functionDefinitionList>
