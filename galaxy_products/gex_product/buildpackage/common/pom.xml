                    </execution>
                    <execution>
                        <id>galaxy-ucore-report-component</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <attach>false</attach>
                            <descriptors>
                                <descriptor>src/assembly/assembly-galaxy-core-report-component.xml</descriptor>
                            </descriptors>
                            <formats>
                                <format>zip</format>
                            </formats>
                            <finalName>galaxy-core-report-component</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>galaxy-email-component</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <attach>false</attach>
                            <descriptors>
                                <descriptor>src/assembly/assembly-galaxy-email-component.xml</descriptor>
                            </descriptors>
                            <formats>
                                <format>${archiveExt}</format>
                                <!--<format>dir</format>-->
                            </formats>
                            <finalName>galaxy-email-${distName}-component</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>galaxy-ftp-component</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <attach>false</attach>
                            <descriptors>
                                <descriptor>src/assembly/assembly-galaxy-ftp-component.xml</descriptor>
                            </descriptors>
                            <formats>
                                <format>${archiveExt}</format>
                                <!--<format>dir</format>-->
                            </formats>
                            <finalName>galaxy-ftp-${distName}-component</finalName>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>parse-version</id>
                        <goals>
                            <goal>parse-version</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>attach-artifacts</id>
                        <phase>package</phase>
                        <goals>
                            <goal>attach-artifact</goal>
                        </goals>
                        <configuration>
                            <artifacts>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-buildpackage-res.zip</file>
                                    <classifier>buildpackage-res</classifier>
                                    <type>zip</type>
                                </artifact>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-buildpackage-res-pat.zip</file>
                                    <classifier>buildpackage-res-pat</classifier>
                                    <type>zip</type>
                                </artifact>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-core-${distNameLong}-component.${archiveExt}</file>
                                    <classifier>core-${distNameLong}-component</classifier>
                                    <type>${archiveExt}</type>
                                </artifact>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-core-component.zip</file>
                                    <classifier>core-component${oemClassifier}</classifier>
                                    <type>zip</type>
                                </artifact>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-core-report-component.zip</file>
                                    <classifier>core-report-component</classifier>
                                    <type>zip</type>
                                </artifact>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-email-${distName}-component.${archiveExt}</file>
                                    <classifier>email-${distName}-component</classifier>
                                    <type>${archiveExt}</type>
                                </artifact>
                                <artifact>
                                    <file>${project.build.directory}/galaxy-ftp-${distName}-component.${archiveExt}</file>
                                    <classifier>ftp-${distName}-component</classifier>
                                    <type>${archiveExt}</type>
                                </artifact>
                            </artifacts>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

