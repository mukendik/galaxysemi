    <build>
        <sourceDirectory>${project.basedir}/src</sourceDirectory>
        <plugins>
            <plugin>
                <artifactId>maven-resources-plugin</artifactId>
                <executions>
                    <execution>
                        <id>copy-resources</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/${basepath}</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${project.basedir}/..</directory>
                                    <excludes>
                                        <exclude>common/**/*</exclude>
                                        <exclude>devdir/**/*</exclude>
                                        <exclude>examinator/**/*</exclude>
                                        <exclude>patman/**/*</exclude>
                                        <exclude>yieldman/**/*</exclude>
                                        <exclude>GS-License-Activation-Utility/**/*</exclude>
                                        <exclude>testermonitor/**/*</exclude>
                                        <exclude>target/**/*</exclude>
                                        <exclude>pom.xml</exclude>
                                        <exclude>src/assembly/**/*</exclude>
                                    </excludes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>unpack-dependencies</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack-dependencies</goal>
                        </goals>
                        <configuration>
                            <excludeArtifactIds>libR${oemExcludeArtifactIdsSuffix}</excludeArtifactIds>
                            <includeTypes>tar.gz,zip</includeTypes>
                            <outputDirectory>${project.build.directory}</outputDirectory>
                            <excludeTransitive>true</excludeTransitive>
                        </configuration>
                    </execution>
                    <!-- for libR -->
                    <execution>
                        <id>unpack-R-lib</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <overWriteSnapshots>true</overWriteSnapshots>
                            <overWriteReleases>true</overWriteReleases>
                            <artifactItems>
                                <artifactItem>
                                    <outputDirectory>${project.build.directory}/other_libraries/R/lib/${distName}</outputDirectory>
                                    <groupId>com.galaxysemi</groupId>
                                    <artifactId>libR</artifactId>
                                    <classifier>${distNameLong}-lib</classifier>
                                    <type>${archiveExt}</type>
                                    <overWrite>true</overWrite>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
                    <!-- for libR -->
                    <execution>
                        <id>unpack-R-home</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <overWriteSnapshots>true</overWriteSnapshots>
                            <overWriteReleases>true</overWriteReleases>
                            <artifactItems>
                                <artifactItem>
                                    <outputDirectory>${project.build.directory}/other_libraries/R/r_home/${distName}</outputDirectory>
                                    <groupId>com.galaxysemi</groupId>
                                    <artifactId>libR</artifactId>
                                    <!-- The artifact version is optional. If not set, the plugin will attempt to resolve it from the project dependencies and then the dependencyManagement section. -->
                                    <classifier>${distNameLong}-home</classifier>
                                    <type>${archiveExt}</type>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-assembly-plugin</artifactId>
                <executions>
                    <execution>
                        <id>galaxy-buildpackage-res</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <attach>false</attach>
                            <descriptors>
                                <descriptor>src/assembly/assembly-buildpackage-res.xml</descriptor>
                            </descriptors>
                            <finalName>galaxy-buildpackage-res</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>galaxy-buildpackage-res-pat</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <attach>false</attach>
                            <descriptors>
                                <descriptor>src/assembly/assembly-buildpackage-res-pat.xml</descriptor>
                            </descriptors>
                            <finalName>galaxy-buildpackage-res-pat</finalName>
                        </configuration>
                    </execution>
                    <execution>
                        <id>galaxy-core-component</id>
                        <phase>prepare-package</phase>
                        <goals>
                            <goal>single</goal>
                        </goals>
                        <configuration>
                            <appendAssemblyId>false</appendAssemblyId>
                            <attach>false</attach>
                            <descriptors>
                                <descriptor>src/assembly/assembly-galaxy-core-${distNameLong}-component.xml</descriptor>
                            </descriptors>
                            <formats>
                                <format>${archiveExt}</format>
                            </formats>
                            <finalName>galaxy-core-${distNameLong}-component</finalName>
                        </configuration>
                    </execution>
