                                    <environmentVariables></environmentVariables>
                                    <executable>npm</executable>
                                    <arguments>
                                        <argument>install</argument>
                                    </arguments>
                                </configuration>
                            </execution>
                            <execution>
                                <id>release-history-build</id>
                                <phase>process-resources</phase>
                                <goals>
                                    <goal>exec</goal>
                                </goals>
                                <configuration>
                                    <workingDirectory>${project.build.directory}/releaseHistory</workingDirectory>
                                    <environmentVariables></environmentVariables>
                                    <executable>node</executable>
                                    <arguments>
                                        <argument>src/releaseHistory</argument>
                                        <argument>--force</argument>
                                        <argument>--families=["GEX,"YM","PAT","GTM","LM"]</argument>
                                        <argument>-minor="V${parsedVersion.majorVersion}.${parsedVersion.minorVersion}"</argument>
                                        <argument>--releases=["V7.7.0-RC1","V7.7.0-RC1-beta","V7.7.0-RC1-alpha"]</argument>
                                        <argument>--output=${project.build.directory}/galaxy_products/gex_product/bin/help/pages/history_v${parsedVersion.majorVersion}${parsedVersion.minorVersion}.htm</argument>
                                    </arguments>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
