    <build>
        <sourceDirectory>${project.basedir}/src</sourceDirectory>
        <plugins>
            <plugin>
                <artifactId>maven-resources-plugin</artifactId>
                <executions>
                    <execution>
                        <id>copy-sources</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/${basepath}</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${gex.basedir}</directory>
                                    <includes>
                                        <include>*.pro</include>
                                        <include>*.pri</include>
                                        <include>sources/**/*</include>
                                    </includes>
                                    <excludes>
                                        <exclude>sources/target/**/*</exclude>
                                    </excludes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <execution>
                        <id>copy-resources</id>
                        <phase>initialize</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/${gexClientProfile}</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${gex.basedir}/src/main/resources</directory>
                                    <includes>
                                        <include>GalaxySemi/gslog.xml</include>
                                        <include>GalaxySemi/xml/gs_lp.xml</include>
                                    </includes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <execution>
                        <id>copy-plugins-lp</id>
                        <phase>process-classes</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/galaxy_products/gex_product/bin/plugins/lp</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${project.build.directory}/galaxy_products/gex_product/plugins/lib/${distName}</directory>
                                    <includes>
                                        <include>*gs_lp*</include>
                                        <include>*fnp_lp*</include>
                                    </includes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <execution>
                        <id>copy-plugins-db</id>
                        <phase>process-classes</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/galaxy_products/gex_product/bin/plugins/db</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${project.build.directory}/galaxy_products/gex_product/plugins/lib/${distName}</directory>
                                    <includes>
                                        <include>*gexdb_plugin_galaxy*</include>
                                        <include>*gexdb_plugin_medtronic*</include>
                                    </includes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                    <execution>
                        <id>copy-plugins-da</id>
                        <phase>process-classes</phase>
                        <goals>
                            <goal>copy-resources</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}/galaxy_products/gex_product/bin/plugins/da</outputDirectory>
                            <resources>
                                <resource>
                                    <directory>${project.build.directory}/galaxy_products/gex_product/plugins/lib/${distName}</directory>
                                    <includes>
                                        <include>*dagalaxy*</include>
                                    </includes>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>unpack-dependencies</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack-dependencies</goal>
                        </goals>
                        <configuration>
                            <outputDirectory>${project.build.directory}</outputDirectory>
                            <excludeArtifactIds>libR</excludeArtifactIds>
                            <excludeTransitive>true</excludeTransitive>
                            <!--                            <excludeGroupIds>org.r-project.cran</excludeGroupIds> -->
                        </configuration>
                    </execution>
                    <!-- for libR -->
                    <execution>
                        <id>unpack</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>unpack</goal>
                        </goals>
                        <configuration>
                            <overWriteSnapshots>true</overWriteSnapshots>
                            <artifactItems>
                                <artifactItem>
                                    <outputDirectory>${project.build.directory}/other_libraries/R/lib/${distName}</outputDirectory>
                                    <groupId>com.galaxysemi</groupId>
                                    <artifactId>libR</artifactId>
                                    <classifier>${distNameLong}-lib</classifier>
                                    <type>${archiveExt}</type>
                                </artifactItem>
                                <artifactItem>
                                    <outputDirectory>${project.build.directory}/other_libraries/R/include</outputDirectory>
                                    <groupId>com.galaxysemi</groupId>
                                    <artifactId>libR</artifactId>
                                    <classifier>inc</classifier>
                                    <type>zip</type>
                                </artifactItem>
                            </artifactItems>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
